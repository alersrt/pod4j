    "scmDeveloperConnection": "scm:git:git://github.com/alersrt/pod4j.git",
    "invokerPackage": "io.github.alersrt.pod4j.openapi",
    "modelPackage": "io.github.alersrt.pod4j.openapi.model",
    "apiPackage": "io.github.alersrt.pod4j.openapi.api",
    "cleanupOutput": true,
    "library": "okhttp-gson",
